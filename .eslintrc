{
  "globals": { "document": true, "window": true },
  "plugins": ["prettier"],
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "airbnb/base",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "max-classes-per-file": ["error", 3],
    "no-use-before-define": [0],
    "class-methods-use-this": [0],
    "new-cap": [0],
    "no-param-reassign": [1],
    "no-underscore-dangle": [0]
  }
}
